@model DataTable
@using System.Data

@{
    ViewData["Title"] = "User List";
}

<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
        <tr>
            <th>UserID</th>
            <th>UserName</th>
            <th>Email</th>
            <th>MobileNo</th>
            <th>Address</th>
            <th>IsActive</th>
        </tr>
    </thead>
    <tbody>
        @foreach (DataRow row in Model.Rows)
        {
            <tr>
                <td>@row["UserID"]</td>
                <td>@row["UserName"]</td>
                <td>@row["Email"]</td>
                <td>@row["MobileNo"]</td>
                <td>@row["Address"]</td>
                <td>@row["IsActive"]</td>
            </tr>
        }
    </tbody>
</table>
